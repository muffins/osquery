table_name("windows_event_log")
description("Table for searching all recorded Windows event logs.")
schema([
    Column("time", TEXT, "System time at which the event occurred"),
    Column("source", TEXT, "Source or channel of the event"),
    Column("provider_name", TEXT, "Provider name of the event"),
    Column("provider_guid", TEXT, "Provider guid of the event"),
    Column("eventid", INTEGER, "Event ID of the event"),
    Column("task", INTEGER, "Task value associated with the event"),
    Column("level", INTEGER, "The severity level associated with the event"),
    Column("keywords", BIGINT, "A bitmask of the keywords defined in the event"),
    Column("data", TEXT, "Data associated with the event"),
    Column("pid", INTEGER, "Process ID which emitted the event record"),
    Column("tid", INTEGER, "Thread ID which emitted the event record"),
    Column("eid", TEXT, "Event ID", hidden=True),
])
implementation("windows_event_log@genWindowsEventLog")
examples([
  "select * from windows_event_log where source='Security' and eventid=4104 and time like '2018-03-15%'",
  "select * from windows_event_log where source='Security' and eventid=4688 and time like '2018-03-15%'",
])
